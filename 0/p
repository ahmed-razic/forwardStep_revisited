FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * /

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 1;

boundaryField
{
    inlet
    {
        type        fixedValue;
        value       1;
    }

    outlet
    {
        type        waveTransmissive;
        field       p;
        gamma       1.4;
        fixedInf    1;
        lInf        3;
        value       uniform 1;
    }

    top
    {
        type        symmetryPlane;
    }

    bottom
    {
        type        symmetryPlane;
    }

    obstacle
    {
        type        zeroGradient;
    }

    frontAndBack
    {
        type        empty;
    }
}

// ************************************************************************* //